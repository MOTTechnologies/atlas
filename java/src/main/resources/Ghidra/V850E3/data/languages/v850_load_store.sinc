# SST.B disp7[ep], reg2
:sst.b op0006[ep], R1115 is op0710=0x07 & op0006 & R1115 & ep {
	local addr:4 = op0006 + ep;
	*:1 addr = R1115 : 1;
}
# SLD.H disp8[ep], reg2
:sld.h disp8[ep], R1115 is op0710=0x08 & op0006 & R1115 & ep
